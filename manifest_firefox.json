{
  "manifest_version": 2,
  "name": "Custom New Tab Manager",
  "version": "1.0.0",
  "description": "A simple and elegant new tab page customizer that enhances your browsing experience with beautiful layouts and quick access features.",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "author": "XMP",
  "homepage_url": "https://github.com/tuananh93cp/custom-new-tab-manager",
  "applications": {
    "gecko": {
      "id": "custom-new-tab-manager@xmp.dev",
      "strict_min_version": "57.0"
    }
  }
} 